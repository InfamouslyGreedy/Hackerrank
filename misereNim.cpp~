#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int t = 0;
    scanf("%d", &t);
    while(t--)
    {
        int n = 0;
        scanf("%d", &n);
        int vals[n];
        int res = 0;
        for(int i = 0; i < n; i++)
        {
            scanf("%d", &vals[i]);
            res = res xor vals[i];
        }
        if(res == 0)
            printf("First\n");
        else
            printf("Second\n");
       
    }
    return 0;
}
