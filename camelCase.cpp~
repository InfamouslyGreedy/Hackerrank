#include <vector>
#include <climits>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>

using namespace std;


int main(){
    string s;
    cin >> s;
    int size = s.size();
    int res = 1;
    for(int i = 0; i < size; i++)
    {
        if((int)s[i] >= 65 && (int)s[i] <= 90)
            res++;
    }
    cout << res << endl;
    return 0;
}
