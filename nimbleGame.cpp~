#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int t = 0;
    scanf("%d", &t);
    while(t--)
    {
        int n = 0;
        scanf("%d", &n);
        int sum = 0;
        for(int i = 0; i < n; i++)
        {
            int curr = 0;
            scanf("%d", &curr);
            sum += curr;
        }
        if(sum % 2 == 1)
            printf("%s\n", "First");
        else
            printf("%s\n", "Second");
    }
    return 0;
}
