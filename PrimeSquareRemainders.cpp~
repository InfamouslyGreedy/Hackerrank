#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int getR(int prime, int n)
{
    int low = pow((prime - 1), n);
    int high = pow((prime + 1), n);
    int pSquared = pow(prime,2);
    int r = (low + high)/pSquared;
}
int main() 
{
    return 0;
}

